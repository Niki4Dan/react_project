{
  "name": "@project/webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "types": "tsc --noEmit --project ./tsconfig.json && tsc --noEmit --project ./tsconfig.node.json",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts, .tsx .",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache 'src/**/*.scss'",
    "prettify": "prettier --log-level warn --cache --write src/**/*.{ts,tsx,js,json,yml,scss} !**/{node_modules,dist}/**/*"
  },
  "dependencies": {
    "@project/backend": "workspace:*",
    "@tanstack/react-query": "^5.90.10",
    "@trpc/client": "^11.7.1",
    "@trpc/react-query": "^11.7.1",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "formik": "^2.4.9",
    "formik-validator-zod": "^2.2.0",
    "include-media": "^2.0.0",
    "js-cookie": "^3.0.5",
    "react": "^19.2.3",
    "react-dom": "^19.2.0",
    "react-head": "^3.4.2",
    "react-icons": "^5.5.0",
    "react-infinite-scroller": "^1.2.6",
    "reset-css": "^5.0.2",
    "sass": "^1.94.2",
    "superjson": "^2.2.6",
    "usehooks-ts": "^3.1.1",
    "zod": "^3.19.1"
